<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.davispc10.proprietario_david">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <meta-data
            android:name="DATABASE"
            android:value="cadastrosDB.db" />
        <meta-data
            android:name="VERSION"
            android:value="3" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.example.davispc10.proprietario_david" />

        <activity
            android:name=".activity.TelaListaProprietario"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".activity.Intro"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.TelaCadastroProprietario"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".activity.TelaListaVeiculo"
            android:label="@string/title_activity_tela_lista_veiculo"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".activity.TelaCadastroVeiculo"
            android:label="@string/title_activity_tela_lista_veiculo"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>