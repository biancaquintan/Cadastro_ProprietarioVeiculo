<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context="com.example.davispc10.proprietario_david.activity.TelaCadastroProprietario">

    <Button
        android:id="@+id/bAlterar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alterar"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="63dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="145dp"/>

    <TextView
        android:id="@+id/txvNome"
        android:layout_width="68dp"
        android:layout_height="35dp"
        android:layout_marginBottom="438dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="330dp"
        android:text="Nome"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/txvTelefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/telefone"
        android:textAppearance="?android:attr/textAppearanceMedium"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="403dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="296dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/txvEndereco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EndereÃ§o"
        android:textAppearance="?android:attr/textAppearanceMedium"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="358dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="293dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/txvData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data"
        android:textAppearance="?android:attr/textAppearanceMedium"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="299dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="330dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/endereco"
        android:layout_width="235dp"
        android:layout_height="44dp"
        android:layout_gravity="center_horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="345dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="38dp" />

    <Button
        android:id="@+id/bSalvar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/salvar"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="145dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="208dp" />

    <EditText
        android:id="@+id/data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="287dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="84dp" />

    <EditText
        android:id="@+id/telefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="396dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="58dp"  />



    <EditText
        android:id="@+id/nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="438dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="84dp" />

</android.support.constraint.ConstraintLayout>
